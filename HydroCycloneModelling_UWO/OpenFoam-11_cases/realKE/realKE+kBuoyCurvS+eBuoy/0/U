FoamFile
{
    format      binary;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (-2.32 0 0);
    }
    bottomoutlet
    {
        type            zeroGradient;
    }
    topoutlet
    {
        type            zeroGradient;
    }

    wall
    {
        type            noSlip;
    }

}


// ************************************************************************* //
